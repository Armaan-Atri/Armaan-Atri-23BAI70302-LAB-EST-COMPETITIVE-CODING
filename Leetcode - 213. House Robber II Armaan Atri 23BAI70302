class Solution:
    def solve(self,index,nums,dp):
        if index == 0:
            return nums[index]
        if index < 0:
            return 0
        if dp[index] != -1:
            return dp[index]
        
        pick = nums[index] + self.solve(index-2,nums,dp)
        not_pick = self.solve(index-1,nums,dp)
        dp[index] = max(pick,not_pick)
        return dp[index]

    def rob(self, nums: List[int]) -> int:
        n = len(nums)
        dp = [-1] * (n-1)
        ans1 = self.solve(n-2,nums[:n-1],dp)
        dp = [-1] * (n-1)
        ans2 = self.solve(n-2,nums[1:n],dp)
        return max(ans1,ans2)

